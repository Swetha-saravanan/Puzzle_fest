<div class="reports">
<% logged_user = User.find(session[:id]) %>
<% if logged_user %>
<div class="dashboard">Swetha</div>
<div class="reports_table">
<table class="table table-hover">

  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
  <%Report.all.each do |t|%>
    <tr>
      <th scope="row">1</th>
      <td><%=t.score%></td>
      <td><%= t.assessments_id%></td>
      <td><%=t.id%></td>
    </tr>

    <%end%>
  </tbody>
</table>
</div>
<%else%>
<%end%>
